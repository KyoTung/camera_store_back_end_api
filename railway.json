{
    "build": {
        "builder": "DOCKERFILE",
        "dockerfilePath": "Dockerfile",
        "config": {
            "phpVersion": "8.2"
        }
    },
    "deploy": {

        "restartPolicyType": "ON_FAILURE",

        "healthcheckPath": "/health.php",
        "healthcheckTimeout": 30,
        "startCommand": "php artisan serve --host=0.0.0.0 --port=$PORT"
    }

}
