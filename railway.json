{
    "build": {
        "builder": "DOCKERFILE",
        "dockerfilePath": "Dockerfile",
        "config": {
            "phpVersion": "8.2"
        }
    },
    "deploy": {
        "startCommand": "php artisan serve --host=0.0.0.0 --port=8000",
        "healthcheckPath": "/",
        "restartPolicyType": "ON_FAILURE"
    }
}
